{
  "name": "apidoc-ui",
  "version": "0.2.0",
  "description": "Apidoc UI",
  "scripts": {
    "build": "webpack --config webpack.config.prod.js",
    "deploy": "DEPLOY=true webpack --config webpack.config.prod.js",
    "start": "webpack-dev-server --config webpack.config.dev.js --hot --inline --history-api-fallback --output-public-path / --progress --colors --host 0.0.0.0",
    "start2": "NODE_ENV=development webpack-dev-server --config webpack2.js --hot --progress --colors",
    "lint": "eslint src",
    "format": "eslint src --fix",
    "lint:style": "stylelint \"src/**/*.css\"",
    "lint:style:ci": "stylelint \"src/**/*.css\"",
    "check": "yarn run test && yarn run flow && yarn run lint && yarn run lint:style",
    "flow:cover": "flow coverage --color",
    "flow:coverage": "flow-coverage-report -i 'src/**/*.js' -x 'src/**/*.test.js' -x 'src/generated/**/*' -t text",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/movio/apidoc-ui"
  },
  "keywords": [
    "movio",
    "apidoc"
  ],
  "contributors": [
    {
      "name": "Kalman Bekesi",
      "url": "https://github.com/kalmanb"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.3.2",
    "babel-plugin-flow-react-proptypes": "^0.21.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-1": "^6.22.0",
    "babel-runtime": "^6.23.0",
    "clean-webpack-plugin": "^0.1.15",
    "css-loader": "^0.26.1",
    "cssnano": "^3.9.1",
    "empty": "^0.10.1",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.10.0",
    "flow-bin": "^0.39.0",
    "flow-coverage-report": "^0.3.0",
    "html-webpack-plugin": "^2.28.0",
    "http-server": "^0.9.0",
    "identity-obj-proxy": "^3.0.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jest": "^19.0.2",
    "json-loader": "^0.5.4",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.1",
    "postcss-reporter": "^3.0.0",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "react-test-renderer": "^15.4.2",
    "rucksack-css": "^0.9.1",
    "style-loader": "^0.13.1",
    "stylelint": "^7.9.0",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-md5-hash": "^0.0.5",
    "webpack-s3-plugin": "^0.9.2"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "immutable": "^3.8.1",
    "lodash": "^4.17.2",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-markdown": "^2.4.4",
    "react-motion": "^0.4.7",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-saga": "^0.14.3",
    "superagent": "^3.5.0"
  }
}
