{
  "name": "apidoc-ui",
  "version": "0.1.0",
  "description": "Apidoc UI",
  "scripts": {
    "start": "webpack-dev-server --hot --inline --history-api-fallback --output-public-path / --progress --colors --host 0.0.0.0",
    "test": "karma start --single-run; exit 0",
    "test:ci": "karma start --single-run",
    "test:watch": "karma start --auto-watch",
    "lint": "eslint src; exit 0",
    "lint:ci": "eslint src",
    "format": "eslint src --fix; exit 0",
    "lint:style": "stylelint src/**/*.css; exit 0",
    "lint:style:ci": "stylelint src/**/*.css",
    "check": "npm run lint:ci && npm run lint:style:ci && npm run test:ci"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/movio/apidoc-ui"
  },
  "keywords": [
    "movio",
    "apidoc"
  ],
  "contributors": [
    {
      "name": "Kalman Bekesi",
      "url": "https://github.com/kalmanb"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-istanbul-loader": "^0.1.0",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-movio": "^1.2.1",
    "babel-runtime": "^6.9.2",
    "carte-blanche": "^0.2.3",
    "carte-blanche-react-plugin": "^0.2.2",
    "carte-blanche-source-plugin": "^0.1.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "cssnano": "^3.7.1",
    "cssrecipes-defaults": "^0.5.0",
    "cssrecipes-reset": "^0.5.0",
    "eslint": "^2.13.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-import-resolver-webpack": "^0.3.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "5.2.2",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.21.0",
    "http-server": "^0.9.0",
    "json-loader": "^0.5.4",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.4",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.5.3",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss-cssnext": "^2.6.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "postcss-reporter": "^1.3.3",
    "react-addons-test-utils": "^15.1.0",
    "react-hot-loader": "^1.3.0",
    "rucksack-css": "^0.8.6",
    "style-loader": "^0.13.1",
    "stylelint": "^6.6.0",
    "stylelint-config-standard": "^9.0.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "immutable": "^3.8.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-saga": "^0.10.5",
    "superagent": "^2.0.0"
  }
}
